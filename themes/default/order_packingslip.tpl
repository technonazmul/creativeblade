<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN">
<html dir="ltr" lang="{LANGUAGE}"><head>
<meta http-equiv="Content-Type" content="text/html; charset=ISO-8859-1">

<title>{L_text_packingslip}</title>

<link rel="stylesheet" type="text/css" href="{INCURL}themes/{THEME}/css/bootstrap.css">
<style type="text/css">
@media print {
  a[href]:after {
    content: none !important;
  }
}.input[type=button] {
	-moz-box-shadow:inset 0px 1px 0px 0px #caefab;
	-webkit-box-shadow:inset 0px 1px 0px 0px #caefab;
	box-shadow:inset 0px 1px 0px 0px #caefab;
	background:-webkit-gradient( linear, left top, left bottom, color-stop(0.05, #77d42a), color-stop(1, #5cb811) );
	background:-moz-linear-gradient( center top, #77d42a 5%, #5cb811 100% );
	filter:progid:DXImageTransform.Microsoft.gradient(startColorstr='#77d42a', endColorstr='#5cb811');
	background-color:#77d42a;
	-moz-border-radius:6px;
	-webkit-border-radius:6px;
	border-radius:6px;
	border:1px solid #268a16;
	display:inline-block;
	color:#306108;
	font-family:arial;
	font-size:13px;
	font-weight:bold;
	padding:6px 10px;
	text-decoration:none;
	text-shadow:1px 1px 0px #aade7c;	cursor: pointer;
}
.input[type=button]:hover {
	background:-webkit-gradient( linear, left top, left bottom, color-stop(0.05, #5cb811), color-stop(1, #77d42a) );
	background:-moz-linear-gradient( center top, #5cb811 5%, #77d42a 100% );
	filter:progid:DXImageTransform.Microsoft.gradient(startColorstr='#5cb811', endColorstr='#77d42a');
	background-color:#5cb811;
}
</style >
</head>
<body topmargin="0" bottommargin="0" leftmargin="0" rightmargin="0" bgcolor="#ffffff" marginheight="0" marginwidth="0">
<!-- IF ORDERS ne '' --> 



<table width="830px" border="0">
  <tbody><tr>
    <td><table width="100%" border="0">
        <tbody><tr>
          <td width="350">
<table width="100%" border="0" cellpadding="0" cellspacing="0" height="100%">
              <tbody><tr>
                <td valign="top" align="center">{LOGO}</td>
              </tr>
              
            </tbody></table></td>
          <td>&nbsp;</td>
          <td valign="top" width="350" align="right">
            </td>
        </tr>
      </tbody></table></td>
  </tr>
  <tr>
    <td><table width="100%" border="0" cellpadding="0" cellspacing="0">
        <tbody><tr>
          <td><hr></td>
          <td class="pageHeading" valign="middle" width="250px" align="center"><em><b>{L_text_packingslip}</b></em></td>
          <td width="10%"> <hr></td>
        </tr>
      </tbody></table></td>
  </tr>
  <tr>
    <td><table width="100%" border="0">
        <tbody><tr>
          <td valign="top" width="350" align="left">
            <table width="100%" border="0" cellpadding="0" cellspacing="0">
              <tbody><tr>
                <td width="11"> <img src="{INCURL}themes/{THEME}/img/mainwhite_01.gif" alt="" width="11" height="16"></td>
                <td background="{INCURL}themes/{THEME}/img/mainwhite_02.gif"> <img src="{INCURL}themes/{THEME}/img/mainwhite_02.gif" alt="" width="24" height="16"></td>
                <td width="19"> <img src="{INCURL}themes/{THEME}/img/mainwhite_03.gif" alt="" width="19" height="16"></td>
              </tr>
              <tr>
                <td background="{INCURL}themes/{THEME}/img/mainwhite_04.gif"> <img src="{INCURL}themes/{THEME}/img/mainwhite_04.gif" alt="" width="11" height="21"></td>
                <td align="center" bgcolor="#ffffff"> <table class="main" width="100%" border="0" cellpadding="0" cellspacing="0">
                    <tbody><tr>
                      <td valign="top" align="left"><b>{L_text_to}</b></td>
                    </tr>
                    <tr>
                      <td valign="bottom" align="left"><img src="{INCURL}themes/{THEME}/img/pixel_trans.gif" alt="" width="1" border="0" height="5"></td>
                    </tr>
                    <tr>
                      <td>{SENDER}</td>
                    </tr>
                    <tr>
                      <td>&nbsp;</td>
                    </tr>
                    <tr>
                      <td><img src="{INCURL}themes/{THEME}/img/pixel_trans.gif" alt="" width="1" border="0" height="1"></td>
                    </tr>                    
                    <tr>
                      <td>&nbsp;</td>
                    </tr>
                  </tbody></table></td>
                <td background="{INCURL}themes/{THEME}/img/mainwhite_06.gif"> <img src="{INCURL}themes/{THEME}/img/mainwhite_06.gif" alt="" width="19" height="21"></td>
              </tr>
              <tr>
                <td> <img src="{INCURL}themes/{THEME}/img/mainwhite_07.gif" alt="" width="11" height="18"></td>
                <td background="{INCURL}themes/{THEME}/img/mainwhite_08.gif"> <img src="{INCURL}themes/{THEME}/img/mainwhite_08.gif" alt="" width="24" height="18"></td>
                <td> <img src="{INCURL}themes/{THEME}/img/mainwhite_09.gif" alt="" width="19" height="18"></td>
              </tr>
            </tbody></table></td>
          <td>&nbsp;</td>
          <td valign="top" width="350" align="right">
            <table width="100%" border="0" cellpadding="0" cellspacing="0">
              <tbody><tr>
                <td width="11"> <img src="{INCURL}themes/{THEME}/img/mainwhite_01.gif" alt="" width="11" height="16"></td>
                <td background="{INCURL}themes/{THEME}/img/mainwhite_02.gif"> <img src="{INCURL}themes/{THEME}/img/mainwhite_02.gif" alt="" width="24" height="16"></td>
                <td width="19"> <img src="{INCURL}themes/{THEME}/img/mainwhite_03.gif" alt="" width="19" height="16"></td>
              </tr>
              <tr>
                <td background="{INCURL}themes/{THEME}/img/mainwhite_04.gif"> <img src="{INCURL}themes/{THEME}/img/mainwhite_04.gif" alt="" width="11" height="21"></td>
                <td align="center" bgcolor="#ffffff"> <table class="main" width="100%" border="0" cellpadding="0" cellspacing="0">
                    <tbody><tr>
                      <td valign="top" align="left"><b>{L_text_ship_to}</b></td>
                    </tr>
                    <tr>
                      <td valign="bottom" align="left"><img src="{INCURL}themes/{THEME}/img/pixel_trans.gif" alt="" width="1" border="0" height="5"></td>
                    </tr>
                    <tr>
                      <td valign="bottom" align="left">{WINNER}</td>
                    </tr>
                    <tr>
                      <td valign="bottom" align="left">&nbsp;</td>
                    </tr>
                   </tbody></table></td>
                <td background="{INCURL}themes/{THEME}/img/mainwhite_06.gif"> <img src="{INCURL}themes/{THEME}/img/mainwhite_06.gif" alt="" width="19" height="21"></td>
              </tr>
              <tr>
                <td> <img src="{INCURL}themes/{THEME}/img/mainwhite_07.gif" alt="" width="11" height="18"></td>
                <td background="{INCURL}themes/{THEME}/img/mainwhite_08.gif"> <img src="{INCURL}themes/{THEME}/img/mainwhite_08.gif" alt="" width="24" height="18"></td>
                <td> <img src="{INCURL}themes/{THEME}/img/mainwhite_09.gif" alt="" width="19" height="18"></td>
              </tr>
            </tbody></table>
          </td>
        </tr>
      </tbody></table></td>
  </tr>
  <tr>
    <td><table class="table table-condensed table-striped table-bordered" width="100%" border="0" cellpadding="2" cellspacing="0">
        <tbody><tr>
          <td class="main-payment"><b>{L_text_auction_id}</b><br>{AUCTION_ID}</td>
          <td class="main-payment"><b>{L_text_date_added}</b><br>{CLOSING_DATE}</td>
          <td class="main-payment"><b>{L_entry_payment_method}</b><br>{PAYMENT_METHOD}</td>
          <td class="main-payment"><b>{L_entry_shipping_method}</b><br>{SHIPPING_METHOD}</td>
        </tr>
      </tbody></table></td>
  </tr>
  <tr>
    <td><img src="{INCURL}themes/{THEME}/img/pixel_trans.gif" alt="" width="1" border="0" height="5"></td>
  </tr>
  <tr>
    <td><table class="table table-condensed table-striped table-bordered" width="100%" border="0" cellpadding="3" cellspacing="0">
      <tbody><tr class="dataTableHeadingRow">
        <td class="dataTableHeadingContent-invoice" colspan="3"><b>{L_column_product}</b></td>
      </tr>
      <tr class="dataTableRow">
        <td class="dataTableContent" valign="top" align="right">
        <!-- IF B_DIGITAL -->
        {DIGITAL_ITEM_QUANTITY} &nbsp;x
        <!-- ELSE -->
        {ITEM_QUANTITY}&nbsp;x
        <!-- ENDIF --></td>
        <td class="dataTableContent" valign="top" style="width: 330px">{AUCTION_TITLE}
        <br><nobr>&nbsp;<i> - {SUBTITLE}</i></nobr>
       </td>
       <!-- IF B_DIGITAL -->
       <td align="center"><a href="{DOWNLOAD}"><img src="{SITE_URL}images/download.gif"></a></td>
      <!-- ELSE -->
      <td></td>
      <!-- ENDIF -->
      </tr>
      <!-- IF B_DUCH-->
     <tr>
        <td ></td>
        <td style="width: 330px" ></td>
        <td class="dataTableRow dataTableContent" style="float:right" >{L_457}: {BIDF}
			<br>{L_30_0110}: {SHIPPING}
			<br>{L_350_1008}: {DADDITIONAL_SHIPPING_COST}
			<br>{L_284}: {QTY}
			<br>{L_350_562}: {DTOTAL}
	       </td>
      </tr>
<!-- ENDIF -->
<!-- IF B_STANDARD -->
     <tr>
        <td ></td>
        <td style="width: 330px" ></td>
        <td class="dataTableRow dataTableContent" style="float:right;" >{L_457}: {BIDF}
			<br>{L_30_0110}: {SHIPPING}
			<!-- IF B_BUY_NOW_ONLY -->
			<br>{L_350_1008}: {ADDITIONAL_SHIPPING_COST}
			<!-- ENDIF -->
			<br>{L_284}: {QTY}
			<!-- IF B_BUY_NOW_ONLY -->
			<br>{L_350_562}: {ATOTAL}
			<!-- ELSE -->
			<br>{L_350_562}: {TOTAL}
			<!-- ENDIF -->
	       </td>
      </tr>
<!-- ENDIF -->
<!-- IF B_DIGITAL -->
	<tr>
        <td ></td>
        <td style="width: 330px" ></td>
        <td class="dataTableRow dataTableContent" style="float:right;" >{L_457}: {BIDF}
			<br>{L_30_0110}: {DIGITAL_ITEM_SHIPPING}
			<br>{L_284}: {DIGITAL_ITEM_QUANTITY}
			<br>{L_350_562}: {BIDF}
	       </td>
      </tr>
<!-- ENDIF -->
      </tbody></table></td>
  </tr>
  <tr>
    <td><img src="{INCURL}themes/{THEME}/img/pixel_trans.gif" alt="" width="1" border="0" height="5"></td>
  </tr>
  <tr>
    <td><table width="100%" border="0" cellpadding="2" cellspacing="0">
        </table></td>
  </tr>
  <tr>
    <td><img src="{INCURL}themes/{THEME}/img/pixel_trans.gif" alt="" width="1" border="0" height="5"></td>
  </tr>
  <tr>
    <td colspan="2" class="main" align="center"><b>{L_entry_thankyou}</b></td>
  </tr>
  <tr>
  <td align="center"> <br>
<FORM>
<INPUT class="input" TYPE="button" onClick="window.print()" value="Print Packing Slip">
</FORM></td>
  </tr>
  <tr>
    <td><img src="{INCURL}themes/{THEME}/img/pixel_trans.gif" alt="" width="1" border="0" height="5"></td>
  </tr>
</tbody>
</table>

<!-- ELSE -->

<div style="position: absolute; top: 15%; left: 40%;">
<h4>{L_entry_no_orders_selected}</h4>
</div>
<!-- ENDIF -->

</body>
</html>